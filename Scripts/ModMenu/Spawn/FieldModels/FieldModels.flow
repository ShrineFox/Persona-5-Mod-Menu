import("FieldModels.msg");

void SpawnFieldModelsMenu()
{
    while (true && !PAD_CHK_PRESS(PadButton.L2))
    {
        int selection = DescriptionMenu(Title_FieldModels, Options_FieldModels);

        switch (selection)
        {
            case -1:
                return;
            case 0: // Spawn Item
                SpawnItemModel(true);
                break;
            case 1: // Spawn Object
                SpawnObjectModel(true);
                break;
        }
    }
}