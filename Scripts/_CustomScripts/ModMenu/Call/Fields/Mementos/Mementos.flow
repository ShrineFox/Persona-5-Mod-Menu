import("Mementos.msg");

void MemeFloorMenu()
{
    int area = NameMenu(Title_MAreas, Opt_MAreas);
    if (area != -1)
    {
        int floor = NumberMenu(2, Title_MemeFloor);

        // Fix to account for Da'at being at end of area list
        area += 1;
        if (area == 9)
            area = 10;
        if (area == 10)
            area = 11;
        if (area == 11)
            area = 9;

        if (floor != -1)
            CALL_AT_DUNGEON(area, floor, 0);
    }
}