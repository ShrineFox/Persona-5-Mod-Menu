void DisableAllBits() {
    for ( int i = 0; i < 8960; i++ ) {
        BIT_OFF( i );
    }
}

void EnableAllBits() {
    for ( int i = 0; i < 8960; i++ ) {
        BIT_ON( i );
    }
}

void ToggleAllBits() {
    for ( int i = 0; i < 8960; i++ ) {
        if (BIT_CHK(i)) {
            BIT_OFF(i);
        }
        else {
            BIT_ON(i);
        }
    }
}

void PrintEnabledBits() {
    for ( int i = 0; i < 8960; i++ ) {
        if (BIT_CHK(i)) {
            PUT(i);
        }
    }
}

void ToggleRange() {
    DisplayMessagePrompt( FlagRangePrompt );
    int range1 =  SelectNumber( 4 );
    DisplayMessagePrompt( FlagRangePrompt2 );
    int range2 = SelectNumber( 4 );
    
    int onOff = SEL_GENERIC_NOT_HELP( -1, SelectOnOff );
    
    switch (onOff) 
    {
        case -1: // Back
            break;
        case 0: // On
            for ( int i = range1; i < range2; i++ ) 
            {
                BIT_ON( i );
            }
            break;
        case 1: // Off
            for ( int i = range1; i < range2; i++ ) 
            {
                BIT_OFF( i );
            }
            break;
    }
}

